{
    "editor.hideCursorInOverviewRuler": false,
    "editor.cursorStyle": "line",
    "editor.overtypeCursorStyle": "block",
    "editor.fontSize": 14
}

{
  "printWidth": 80,
  "tabWidth": 2,
  "useTabs": false,
  "semi": true,
  "singleQuote": true,
  "trailingComma": "es5",
  "bracketSpacing": true,
  "jsxBracketSameLine": false,
  "arrowParens": "avoid",
  "proseWrap": "always"
}